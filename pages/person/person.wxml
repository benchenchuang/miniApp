<page-navigation is-back="{{true}}"></page-navigation>
<text>pages/person/person.wxml</text>

<swiper
  indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}"
  interval="{{interval}}"
  duration="{{duration}}" vertical="true" circular="true">
